{
  "$schema": "../../schema/scaffold.schema.json",
  "description": "A file scaffold for modules",
  "expected-structure": [
    "/",
    "└── src/",
    "│   ├── _internals/",
    "│   │   ├── _constants.ts",
    "│   │   ├── _utils.ts",
    "│   │   └── mod.ts",
    "│   ├── exceptions/",
    "│   │   └── mod.ts",
    "│   ├── types/",
    "│   │   ├── enums.ts",
    "│   │   ├── interfaces.ts",
    "│   │   ├── mod.ts",
    "│   │   └── types.ts",
    "│   ├── constants.ts",
    "│   ├── mod.ts",
    "│   └── version.ts",
    "├── tests/",
    "├── deps.ts",
    "├── dev_deps.ts",
    "└── mod.ts"
  ],
  "root": {
    "folders": {
      "src": {
        "folders": {
          "_internals": {
            "files": {
              "_constants.ts": {
                "template": "other-template",
                "data": {
                  "description": "Internal constants for use with the partic11e library {{module}} module."
                }
              },
              "_utils.ts": {
                "template": "other-template",
                "data": {
                  "description": "Internal utilities for use with the partic11e library {{module}} module."
                }
              },
              "mod.ts": {
                "template": "mod-template",
                "data": {
                  "description": "Internal features for use with the partic11e library {{module}} module."
                },
                "regions": {
                  "export-constants": [
                    "export * from \"./_constants.ts\";"
                  ],
                  "export-features": [
                    "export * from \"./_utils.ts\";"
                  ]
                }
              }
            }
          },
          "exceptions": {
            "files": {
              "mod.ts": {
                "template": "mod-template",
                "data": {
                  "description": "The module-specific exceptions for the partic11e library {{module}} module."
                }
              }
            }
          },
          "types": {
            "files": {
              "enums.ts": {
                "template": "other-template",
                "data": {
                  "description": "The global shared enums for the partic11e library {{module}} module."
                }
              },
              "interfaces.ts": {
                "template": "other-template",
                "data": {
                  "description": "The global shared interfaces for the partic11e library {{module}} module."
                }
              },
              "mod.ts": {
                "template": "mod-template",
                "data": {
                  "description": "The global shared enums, interfaces, and types for the partic11e library {{module}} module."
                },
                "regions": {
                  "export-types": [
                    "export * from \"./interfaces.ts\";",
                    "export * from \"./types.ts\";"
                  ],
                  "export-constants": [
                    "export * from \"./enums.ts\";"
                  ]
                }
              },
              "types.ts": {
                "template": "other-template",
                "data": {
                  "description": "The global shared types for the partic11e library {{module}} module."
                }
              }
            }
          }
        },
        "files": {
          "constants.ts": {
            "template": "other-template",
            "data": {
              "description": "Public API constants for use with the partic11e library {{module}} module."
            }
          },
          "mod.ts": {
            "template": "mod-template",
            "data": {
              "description": "Source-level exports of the public API for the partic11e library {{module}} module."
            },
            "regions": {
              "export-types": [
                "export * from \"./types/mod.ts\";"
              ],
              "export-constants": [
                "export * from \"./constants.ts\";",
                "export * from \"./version.ts\";"
              ],
              "export-features": [
                "export * from \"./exceptions/mod.ts\";"
              ]
            }
          },
          "version.ts": {
            "template": "version-template",
            "data": {
              "version": "0.0.0"
            }
          }
        }
      },
      "tests": {
      }
    },
    "files": {
      "deps.ts": {
        "template": "version-template",
        "data": {
          "description": "Dependencies for the partic11e library {{module}} module."
        },
        "regions": {
          "export-features": [
            "export * from \"https://denopkg.com/partci11e/exceptions@dev/mod.ts\";"
          ]
        }
      },
      "dev_deps.ts": {
        "template": "mod-template",
        "data": {
          "description": "Development dependencies for the partic11e library {{module}} module."
        }
      },
      "mod.ts": {
        "template": "mod-template",
        "data": {
          "description": "Module-level exports of the public API for the partic11e library {{module}} module."
        },
        "regions": {
          "export-features": [
            "export * from \"./src/mod.ts\";"
          ]
        }
      }
    }
  }
}
